{
  "name": "env-yaml-webpack-plugin",
  "version": "0.1.2",
  "description": "A simple webpack plugin to support env-yaml",
  "main": "index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist src",
    "prebuild": "rimraf dist",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrwells/env-yaml-webpack-plugin.git"
  },
  "author": "Robert Wells",
  "bugs": {
    "url": "https://github.com/jrwells/env-yaml-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jrwells/env-yaml-webpack-plugin#readme",
  "peerDependencies": {
    "webpack": ">=1.14.0"
  },
  "dependencies": {
    "env-yaml": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  }
}
