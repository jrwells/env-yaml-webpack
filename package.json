{
  "name": "env-yaml-webpack-plugin",
  "version": "0.0.1",
  "description": "A simple webpack plugin to support env-yaml",
  "main": "index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist src",
    "prebuild": "rimraf dist",
    "prepare": "npm run build"
  },
  "author": "Robert Wells",
  "license": "ISC",
  "peerDependencies": {
    "webpack": ">=1.14.0"
  },
  "dependencies": {
    "env-yaml": "github:jrwells/env-yaml"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  }
}
